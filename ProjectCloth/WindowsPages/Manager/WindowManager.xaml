<Window x:Class="ProjectCloth.WindowsPages.Manager.WindowManager"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectCloth.WindowsPages.Manager"
        mc:Ignorable="d"
        Title="WindowManager" Height="450" Width="800">
    <Grid>

        <Grid.RowDefinitions>

            <RowDefinition Height="1.5*"/>
            <RowDefinition Height="10*"/>

        </Grid.RowDefinitions>

        <Button Name="Btn_Back" Style="{StaticResource BtnDefault}" Content="Назад" Click="Btn_Back_Click" HorizontalAlignment="Left" VerticalAlignment="Center" Grid.Row="0" Visibility="Hidden"/>

        <Frame Name="FrmWindowManager" NavigationUIVisibility="Hidden" Grid.Row="1"/>

    </Grid>

</Window>
